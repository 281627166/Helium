<?xml version="1.0" encoding="UTF-8"?>
<joblist>
    <namespace name="Helium" />
    <moduletokenprefix name="HELIUM_" />

    <job name="FibJob">
        <parameters>
            <input name="n" type="uint32_t" default="0" />
            <inout name="pSum" type="uint32_t*" default="NULL" />
        </parameters>
    </job>

    <job name="FibContinuation">
        <parameters>
            <input name="x" type="uint32_t" default="0" />
            <input name="y" type="uint32_t" default="0" />
            <inout name="pSum" type="uint32_t*" default="NULL" />
        </parameters>
    </job>
</joblist>
